scan 'songdata', {COLUMNS => ['data:title' ,'data:artist_name', 'data:duration', 'data:year'], FILTER => "SingleColumnValueFilter('data', 'artist_name', = , 'regexstring:^(Bob Marley)') AND SingleColumnValueFilter('data', 'duration', > , 'binary:400') AND SingleColumnValueFilter('data', 'year', >= , 'binary:2000')"}


