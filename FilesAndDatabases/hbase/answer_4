scan 'songdata', {COLUMNS => ['data:title' ,'data:artist_name', 'data:artist_hotttnesss'], FILTER => "SingleColumnValueFilter('data', 'artist_name', = , 'regexstring:Consequence') AND SingleColumnValueFilter('data', 'title', = , 'regexstring:Family') AND SingleColumnValueFilter('data', 'artist_hotttnesss', >= , 'binary:1')"}
